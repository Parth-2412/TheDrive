{
  "name": "frontend-workspace",
  "version": "1.0.0",
  "description": "Frontend workspace with Vite app and React File Manager component",
  "private": true,
  "workspaces": [
    "app",
    "react-file-manager"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=app",
    "build": "npm run build --workspace=app",
    "preview": "npm run preview --workspace=app",
    "lint": "npm run lint --workspace=app",
    "type-check": "npm run type-check --workspace=app",
    "build:all": "npm run build --workspaces --if-present",
    "dev:file-manager": "npm run dev --workspace=react-file-manager",
    "build:file-manager": "npm run build --workspace=react-file-manager",
    "install:all": "npm install --workspaces",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "setup": "./setup.sh"
  },
  "keywords": [
    "frontend",
    "workspace",
    "vite",
    "react",
    "file-manager"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.21",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "dependencies": {
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  }
}
